{
  "name": "nettrine/extensions-atlantic18",
  "description": "Doctrine2 behavioral extensions for Nette Framework",
  "keywords": ["nette", "doctrine", "orm", "database"],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/doctrine-extensions-atlantic18",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=8.3",
    "nette/di": "^3.0.0",
    "doctrine/orm": "^3.0",
    "doctrine/annotations": "^2.0",
    "gedmo/doctrine-extensions": "^3.19"
  },
  "require-dev": {
    "nettrine/orm": "^0.10",
    "ninjify/qa": "^0.13",
    "ninjify/nunjuck": "^0.4",
    "phpstan/phpstan": "^2.0",
    "phpstan/phpstan-deprecation-rules": "^2.0",
    "phpstan/phpstan-nette": "^2.0",
    "phpstan/phpstan-strict-rules": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Nettrine\\Extensions\\Atlantic18\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Cases\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.7.x-dev"
    }
  }
}
