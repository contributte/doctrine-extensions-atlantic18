{
  "name": "nettrine/extensions-atlantic18",
  "description": "Doctrine2 behavioral extensions for Nette Framework",
  "keywords": ["nette", "doctrine", "orm", "database"],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/doctrine-extensions-atlantic18",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=8.2",
    "doctrine/orm": "^3.5.8",
    "gedmo/doctrine-extensions": "^3.22.0",
    "nette/di": "^3.2.5"
  },
  "require-dev": {
    "contributte/phpstan": "^0.2.0",
    "contributte/qa": "^0.4.0",
    "contributte/tester": "^0.3.0",
    "nettrine/orm": "^0.10.0"
  },
  "autoload": {
    "psr-4": {
      "Nettrine\\Extensions\\Atlantic18\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.8.x-dev"
    }
  }
}
