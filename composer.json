{
  "name": "nettrine/extensions-atlantic18",
  "description": "Doctrine2 behavioral extensions for Nette Framework",
  "keywords": ["nette", "doctrine", "orm", "database"],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/nettrine/extensions-atlantic18",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    },
    {
      "name": "Josef Benjac",
      "homepage": "http://josefbenjac.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "nette/di": "~3.0.0",
    "doctrine/orm": "^2.5.0",
    "doctrine/annotations": "^1.6.0",
    "gedmo/doctrine-extensions": "^2.4.35"
  },
  "require-dev": {
    "nettrine/orm": "^0.5.0",
    "ninjify/qa": "^0.10.0",
    "phpstan/extension-installer": "^1.0",
    "phpstan/phpstan-deprecation-rules": "^0.11",
    "phpstan/phpstan-nette": "^0.11",
    "phpstan/phpstan-shim": "^0.11",
    "phpstan/phpstan-strict-rules": "^0.11",
    "phpunit/phpunit": "^8.1.0"
  },
  "autoload": {
    "psr-4": {
      "Nettrine\\Extensions\\Atlantic18\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Nettrine\\Extensions\\Atlantic18\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.5.x-dev"
    }
  }
}
